#include<bits/stdc++.h>
using namespace std;
int main()
{
    int test;
    cin>>test;
    while(test--)
    {
        int n;
        cin>>n;
        string *a=new string[n];
        for(int i=0;i<n;i++)
        cin>>a[i];
        bool flag=false;
        for(int i=0;i<n;i++)
        {
           if(a[i]=="cookie") 
           {
               flag=true;
               break;
           }
        }
        if(flag)
        {
           if(n==1)
           cout<<"NO"<<endl;
           else if(a[n-1]=="cookie")
           cout<<"NO"<<endl;
           else
           {  flag=true;
               for(int i=0;i<n-1;i++)
               {
                   if(a[i]=="cookie")
                   {
                       if(a[i+1]!="milk")
                       {
                           flag=false;
                           break;
                       }
                   }
               }
               if(flag)
               cout<<"YES"<<endl;
               else
               cout<<"NO"<<endl;
           }
        }
        else
        cout<<"YES"<<endl;
        
        
    }
}
